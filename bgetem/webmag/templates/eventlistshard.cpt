<h4 class="media-heading">Termine</h4>
<ul class="event" tal:define="eventlist shard.banner()">
  <li class="small-blue-event" tal:repeat="event eventlist">
    <strong tal:content="event.get('date')" />, <strong tal:content="event.get('location')" /><br />
    <span tal:content="event.get('eventtext')" />
    <span class="seperator" tal:condition="not repeat.event.end" />
  </li>
</ul>
